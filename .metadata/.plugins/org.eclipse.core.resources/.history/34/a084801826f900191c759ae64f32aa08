package com.class13;

import java.util.Arrays;

public class Repl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] a = {
				{5,2,3,7},
				{1,5,1,1},
				{8,3,1,2}
			};


		
	Arrays.sort(a);
	int[][] b=a;
	
		for(int i=a.length-1; i>=0; i--) {
			for(int j=a.length-1; j>=0; j--) {
				
				System.out.println(b[i][j]+" ");
			}
			
		}
	}

}
