package com.class13;

import java.util.Arrays;

public class Repl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] a = {
				{5,2,3,7},
				{1,5,1,1},
				{8,3,1,2}
			};


		
	Arrays.sort(a);
	
	
	
		for(int i=0; i<4; i++) {
			for(int j=3; j>=0; j--) {
				
				System.out.println(a[i][j]+" ");
			}
			
		}
	}

}
