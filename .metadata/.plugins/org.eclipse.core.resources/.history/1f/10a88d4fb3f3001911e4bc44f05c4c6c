package com.array;

public class Repl1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		 for(int i=1;i<=50; i++){
		      
		      System.out.print(i+" ");
		      System.out.println();
		      if(i%2==0 || i==20){
		         System.out.print(i+" ");
		       System.out.println();  
		        
		      }
		      else if(i%3==0 || i==30){
		         System.out.print(i+" ");
		       System.out.println();  
		      
		    }
		    else if(i%4==0 || i==30){
		         System.out.print(i+" ");
		       System.out.println();  
		}

		else if(i%5==0 || i==500){
		         System.out.print(i+" ");
		       System.out.println();  
		  }
		  
		 }
		
	}

}
