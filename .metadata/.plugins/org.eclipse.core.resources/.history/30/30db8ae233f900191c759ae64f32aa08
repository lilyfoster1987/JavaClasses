package com.class13;

import java.util.Arrays;

public class Repl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] numbers = {
				{-5,-2,-3,7},
				{1,-5,-2, 2},
				{1,-2, 3,-4}
			};
	//should print 3

			int sum = 0;
			for (int j = 0; j < numbers.length; j++) {
				for (int i = 0; i < numbers[j].length; i++) {
					if (i==1) {
						sum +=numbers[j][i];
					}
				}
			}
	     System.out.println(sum);
		}
	
	
	



	

}
