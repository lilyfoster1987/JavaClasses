package com.interwiev;

public class Question6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		String str="zeynep";
		
		int a=str.length();
		boolean result=true;
		
		
		for(int i=0; i<str.length()/2; i++) {
			
			if(str.charAt(i)==str.charAt(a-1)) {
				
				result=true;
			}else {
				
				result=false;
			}
			
				
				
			
			
		}
		
		
	}

}
