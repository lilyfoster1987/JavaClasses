package com.rewiev;

public class Repl1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1; i<=5; i++ ) {
			
			for(int j=1; j<=5; j++) {
				System.out.print("*");
			
				
			if(j==2 && j==3 && i==2 ) {
				
				System.out.print(" ");}
			}
			
			System.out.println();
			}
			
		}
	

}
