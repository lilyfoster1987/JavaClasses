		// TODO Auto-generated method stub

		
		Scanner scan=new Scanner(System.in);
		System.out.println("please enter a number");
		int a=scan.nextInt();
		int b=0;
		while(a!=0) {
			
			
			System.out.print(a%10);
			a/=10;
			
		}

b=a;

System.out.println("***********");
System.out.println(b);
	}
	

