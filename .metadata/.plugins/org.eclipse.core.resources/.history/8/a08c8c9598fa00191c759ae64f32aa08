package com.class13;

import java.util.Scanner;

public class Repl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  
		
		int[][] a = {
				{1,1,1},
				{1,1,1},
				{1,1,1}
			};
	 //should be true

	int row=a.length;
	int column=a[0].length;
	for(int i=0; i<row; i++){
	  
	  for(int j=0; j<a[i].length; j++){
	 
		if(row==column){
		  System.out.println("true");
		}
		else{
		  
		   System.out.println("false");
		}
		  
	  }}
		
	    
	
	
	}

}
