package com.interwiev;



public class Question2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] num= {2,5,8,1,0,9};
		
		int min=0;
		int max=0;
		int secondMax=0;
		for(int i=0; i<num.length; i++) {
			
			if(max>num[i]) {
				
				max=num[i];
			}
			else if(min<num[i]) {
				
				min=num[i];
				
			}
			
			
		}
			
		System.out.println(max);
		System.out.println(min);
	}
}
